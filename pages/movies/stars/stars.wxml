<template name="stars">
  <view class="stars-container">
    <block wx:for="{{stars}}" wx:for-item="i" wx:key="1">
      <image wx:if="{{i}}" class="star" src="/images/icon/star.png"></image>
      <image wx:else class="star" src="/images/icon/none-star.png"></image>
    </block>
    <text class="score">{{score}}</text>
  </view>
</template>